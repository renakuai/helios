{
  "source": [
    "src/tokens/**/*.json"
  ],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "build/scss/",
      "prefix": "token",
      "files": [
        {
          "destination": "_global.scss",
          "format": "scss/variables"
        }
      ]
    }
  }
}